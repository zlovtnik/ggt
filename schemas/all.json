    "total_amount": { "type": "number" },
    "name": { "type": "string" },
    "email": { "type": "string", "format": "email" },
    "items": { "type": "array" },
    "tags": { "type": "array", "items": { "type": "string" } },
    "csv_field": { "type": "string" },
    "raw_text": { "type": "string" },
    "password": { "type": "string" },
    "processed_at": { "type": "string", "format": "date-time" }
